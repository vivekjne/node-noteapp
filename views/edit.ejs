<% include partials/header %>
<div class="container">
<form action="/notes/edit/<%= note._id %>" method="POST">
<div class="form-group">
    <label for="title">Title</label>
    <input type="text" name="title" class="form-control" value="<%= note.title %>">
</div>
<div class="form-group">
    <label for="title">Note</label>
    <textarea rows=10 name="note" class="form-control"><%= note.note %></textarea>
</div>

<input type="submit" value="Save" name="save" class="btn btn-primary btn-block">
</form>
</div>
<% include partials/footer %>
